{
  "functions": {
    "resume-site/index.html": {
      "includeFiles": "**"
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/resume-site/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.(jpg|jpeg|png|gif|webp))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.(css|js))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
} 